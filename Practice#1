import javax.swing.text.html.HTMLDocument;
import java.util.*;

public class CustomList extends AbstractList<Integer> {
Integer size;

    public CustomList(Integer size) {
        this.size = size;
    }

    @Override
    public int size() {
        return this.size;
    }
    @Override
    public Integer get(int index) {
        return index;
    }
    @Override
    public boolean isEmpty() {
        return false;
    }


    public static void main(String[] args) {
        List<Integer> list = new CustomList(5);
        for (int i : list) {
            System.out.println(i);
        }
    }

}
