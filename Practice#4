import java.util.Comparator;

public class ComparatorForStringFromSecondChar implements Comparator<String> {
    @Override
    public int compare(String o1, String o2) {
        if(o1.equals("null")|| o2.equals("null"))
            return 0;
        else if(o1.isEmpty()|| o2.isEmpty())
            return 0;
        else if (o2.substring(1).compareTo(o1.substring(1))>1)
            return 1;
        else if (o2.compareTo(o1)>1)
            return 1;
        else return -1;
    }
}
public class ComparatorForStringFromSecondCharManagement{

    public static void main(String[] args)
    {
        Set<String> fullData=new TreeSet<String>(new WorkPriorityStatus());
        fullData.add("Severe");
        fullData.add("High");
        fullData.add("Low");
       System.out.println("My work priorities in descending order is-- " + fullData);

     }
}
